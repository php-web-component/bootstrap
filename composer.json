{
    "name": "php-web-component/bootstrap",
    "description": "PHP Web Component Bootstrap",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagus A Priambada",
            "email": "iam.adty@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PWC\\": "core/",
            "PWC\\Asset\\": "asset/",
            "PWC\\Component\\Bootstrap\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php-web-component/asset": "dev-develop",
        "php-web-component/html": "dev-develop",
        "php-web-component/jquery": "dev-develop"
    },
    "require-dev": {
        "twbs/bootstrap": "3.4.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://localhost/satis"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "secure-http": false
    }
}
